<H1>Lista uzytkownikow</H1>
<table>
	<tr>
		<td>ID</td>
		<td>Imie</td>
		<td>Nazwisko</td>
		<td>Login</td>
		<td>Email</td>
		<td>Miasto</td>
	</tr>

	<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.imie %></td>
			<td><%= user.nazwisko %></td>
			<td><%= user.login %></td>
			<td><%= user.email %></td>
			<td><%= user.miasto %></td>
		</tr>
	<% end %>
</table>